{
  "feature_store": "fake_data",
  "sanitization": {
    "remove_nulls": [
      "client_id",
      "status_transacao",
      "valor"
    ],
    "exclude": {
      "status_transacao": [
        69,
        420
      ]
    },
    "include_only": {
      "destino": [
        "90090990",
        "24234234"
      ]
    }
  },
  "transformations": {
    "rename": {
      "destino": "estabelecimento",
      "valor": "value"
    },
    "cast": {
      "status_transacao": "string",
      "ano": "float"
    }
  },
  "writing": {
    "partitions": [
        "produto_origem",
        "ano",
        "mes",
        "dia"
      ],
    "saving_path": "S3",
    "saving_format": "parquet",
    "saving_mode": "overwrite"
  }
}